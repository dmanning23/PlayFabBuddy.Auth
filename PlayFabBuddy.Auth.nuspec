<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>PlayFabBuddy.Auth</id>
		<version>2.0.18</version>
		<title>PlayFabBuddy.Auth</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/PlayFabBuddy.Auth</projectUrl>
		<licenseUrl>https://github.com/dmanning23/PlayFabBuddy.Auth/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Authentication wrapper around the PlayFab Xamarin client SDK</description>
		<releaseNotes />
