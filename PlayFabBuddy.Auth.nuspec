		<file src="PlayFabBuddy.Auth.iOS\bin\Release\PlayFabBuddy.Auth.iOS.dll" target="lib\Xamarin.iOS10\PlayFabBuddy.Auth.iOS.dll" />
		
		<!--WindowsDX-->
		<file src="PlayFabBuddy.Auth.WindowsDX\bin\Release\PlayFabBuddy.Auth.WindowsDX.pdb" target="lib\net451\PlayFabBuddy.Auth.WindowsDX.pdb" />
		<file src="PlayFabBuddy.Auth.WindowsDX\bin\Release\PlayFabBuddy.Auth.WindowsDX.dll" target="lib\net451\PlayFabBuddy.Auth.WindowsDX.dll" />

